<?xml version="1.0" encoding="UTF-8"?>
<root>
  <instruction text="move to the bottle before going to the box"/>
  <world>
    <object name="R" type="robot" />
    <object name="B" type="bottle" />
    <object name="C" type="chair" />
    <object name="T" type="table" />
    <object name="BX" type="box" />
  </world>

  <VP>
    <grounding>
      <grounding_set>
        <constraint type="inside">
	  <parent>
	    <region type="na">
	      <object name="R" type="robot"/>
	    </region>
	  </parent>
	  <child>
	    <region type="na">
	      <object name="B" type="bottle"/>
	    </region>
	  </child>
	</constraint>
        <func_kernel weight="0.2">
	  <object name="BX" type="box"/>
        </func_kernel>
      </grounding_set>
    </grounding>
    <VB text="move"/>
    <PP>
      <grounding>
        <grounding_set>
          <region type="na">
            <object name="B" type="bottle"/>
          </region>
        </grounding_set>
      </grounding>
      <TO text="to"/>
      <NP>
        <grounding>
          <grounding_set>
            <region type="na">
              <object name="B" type="bottle"/>
            </region>
          </grounding_set>
        </grounding>
        <DT text="the"/>
	<NN text="bottle"/>
      </NP>
    </PP>
    <PP>
      <grounding>
        <grounding_set>
          <func_kernel weight="0.2">
	    <object name="BX" type="box"/>
          </func_kernel>
        </grounding_set>
      </grounding>
      <IN text="before"/>
      <VP>
        <grounding>
          <grounding_set>
            <func_kernel weight="0.2">
	      <object name="BX" type="box"/>
            </func_kernel>
          </grounding_set>
        </grounding>
        <VBG text="going"/>
        <PP>
          <grounding>
            <grounding_set>
              <func_kernel weight="0.2">
	      	<object name="BX" type="box"/>
              </func_kernel>
            </grounding_set>
          </grounding>
          <TO text="to"/>
          <NP>
            <grounding>
              <grounding_set>
	        <object name="BX" type="box"/>
              </grounding_set>
            </grounding>
            <DT text="the"/>
            <NN text="box"/>
          </NP>
        </PP>
      </VP>
    </PP>
  </VP>
</root>
